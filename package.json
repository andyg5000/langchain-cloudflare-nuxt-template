{
  "name": "langchain-cloudflare-nuxt-template",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NITRO_PRESET=cloudflare-module nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "reload": "npm run build && npm run preview",
    "deploy": "npm run build && npx wrangler deploy .output/server/index.mjs --site .output/public",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@nuxt/devtools": "latest",
    "nuxt": "^3.8.0",
    "prettier": "3.1.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.23",
    "wrangler": "^3.15.0"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.0.33",
    "@mdi/font": "^7.3.67",
    "@microsoft/fetch-event-source": "^2.0.1",
    "cheerio": "^1.0.0-rc.12",
    "langchain": "^0.0.197-rc.1",
    "langsmith": "^0.0.48",
    "pdf-parse": "^1.1.1"
  }
}
